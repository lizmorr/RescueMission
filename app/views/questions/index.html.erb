<header><h1>All Questions</h1><header>

<div class="container">
  <ul id="all_questions">
    <% @questions.each do |question| %>
      <li>
        <a href="questions/<%= question.id %>"><%= question.title %></a>
      </li>
    <% end %>
  </ul>
</div>

<%= link_to "Ask a new question", new_question_path, class: "button radius" %>
